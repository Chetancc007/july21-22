<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\MediaStorage\Model\Config\Source\Storage\Media\Storage">
        <plugin name="Magecomp\S3Amazon\Model\Config\Source\Storage\Media\S3Storage" type="Magecomp\S3Amazon\Model\Config\Source\Storage\Media\S3Storage" />
    </type>
    <type name="Magento\MediaStorage\Model\File\Storage">
        <plugin name="Magecomp\S3Amazon\Model\Core\File\Storage" type="Magecomp\S3Amazon\Model\Core\File\Storage" />
    </type>
    <type name="\Magento\MediaStorage\Model\File\Storage\Database">
        <plugin name="Magecomp\S3Amazon\Model\Core\File\Storage\S3Database" type="Magecomp\S3Amazon\Model\Core\File\Storage\S3Database" />
    </type>
    <type name="\Magento\MediaStorage\Model\File\Storage\Directory\Database">
        <plugin name="Magecomp\S3Amazon\Model\Core\File\Storage\Directory\S3Database" type="Magecomp\S3Amazon\Model\Core\File\Storage\Directory\S3Database" />
    </type>
    <type name="Magento\Cms\Model\Wysiwyg\Images\Storage">
        <plugin name="Magecomp\S3Amazon\Model\Cms\Wysiwyg\Images\S3Storage" type="Magecomp\S3Amazon\Model\Cms\Wysiwyg\Images\S3Storage" />
    </type>
    <type name="Magento\Captcha\Model\DefaultModel">
        <plugin name="Magecomp\S3Amazon\Model\Captcha\S3DefaultModel" type="Magecomp\S3Amazon\Model\Captcha\S3DefaultModel" />
    </type>
    <type name="Magento\MediaStorage\Helper\File\Storage\Database">
        <plugin name="Magecomp\S3Amazon\Helper\Core\File\Storage\S3Database" type="Magecomp\S3Amazon\Helper\Core\File\Storage\S3Database" />
    </type>
    <type name="Magento\Theme\Model\Design\Backend\Logo">
        <plugin name="Magecomp\S3Amazon\Model\Theme\Design\Backend\S3Logo" type="Magecomp\S3Amazon\Model\Theme\Design\Backend\S3Logo" />
    </type>
    <type name="Magento\MediaStorage\Block\System\Config\System\Storage\Media\Synchronize">
        <plugin name="Magecomp\S3Amazon\Block\System\Config\Storage\Media\S3Synchronise" type="Magecomp\S3Amazon\Block\System\Config\Storage\Media\S3Synchronise" />
    </type>
   <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="amazons3storageexport" xsi:type="object">Magecomp\S3Amazon\Model\Command\S3Export</item>
                <item name="amazons3storageenable" xsi:type="object">Magecomp\S3Amazon\Model\Command\S3Enable</item>
                <item name="amazons3storagedisable" xsi:type="object">Magecomp\S3Amazon\Model\Command\S3Disable</item>
            </argument>
        </arguments>
    </type>
    <preference for="Magento\Downloadable\Controller\Download\Link" type="Magecomp\S3Amazon\Controller\Download" />
</config>
