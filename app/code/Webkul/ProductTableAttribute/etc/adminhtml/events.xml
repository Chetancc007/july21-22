<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="adminhtml_product_attribute_types">
        <observer name="webkul.table.attribute" instance="Webkul\ProductTableAttribute\Observer\AddTableAttributeType" />
    </event>
    <event name="catalog_entity_attribute_save_after">
        <observer name="webkul.table.attribute.after.save" instance="Webkul\ProductTableAttribute\Observer\AttributeAfterSave" />
    </event>
    <event name="catalog_entity_attribute_delete_after">
        <observer name="webkul.table.attribute.after.delete" instance="Webkul\ProductTableAttribute\Observer\AttributeAfterDelete" />
    </event>
    <event name="catalog_product_save_before">
        <observer name="webkul.table.catalog.before.save" instance="Webkul\ProductTableAttribute\Observer\CatalogBeforeSave" />
    </event>
</config>
