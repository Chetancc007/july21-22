<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Mageplaza_Core::css/mp.css"/>
        <css src="Mageplaza_Core::css/grid-mageplaza.css"/>
        <css src="Mageplaza_SaveCart::css/dashboard.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\SaveCart\Block\Account\Dashboard" name="mpsavecart_dashboard" template="Mageplaza_SaveCart::account/dashboard.phtml" cacheable="false">
                <block class="Mageplaza\SaveCart\Block\Account\Dashboard\Carts" name="mpsavecart_dashboard_carts" template="Mageplaza_SaveCart::account/dashboard/carts.phtml"/>
                <block class="Mageplaza\SaveCart\Block\Account\Dashboard\Products" name="mpsavecart_dashboard_products" template="Mageplaza_SaveCart::account/dashboard/products.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>
